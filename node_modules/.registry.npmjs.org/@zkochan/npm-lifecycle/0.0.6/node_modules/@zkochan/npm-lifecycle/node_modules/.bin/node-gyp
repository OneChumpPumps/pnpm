#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node-gyp/3.6.2/node_modules/node-gyp/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node-gyp/3.6.2/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node-gyp/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@zkochan/npm-lifecycle/0.0.6/node_modules/node-gyp/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@zkochan/npm-lifecycle/0.0.6/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@zkochan/npm-lifecycle/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@zkochan/node_modules" "$basedir/node"  "$basedir/../../../../node-gyp/bin/node-gyp.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node-gyp/3.6.2/node_modules/node-gyp/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node-gyp/3.6.2/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node-gyp/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@zkochan/npm-lifecycle/0.0.6/node_modules/node-gyp/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@zkochan/npm-lifecycle/0.0.6/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@zkochan/npm-lifecycle/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/@zkochan/node_modules" node  "$basedir/../../../../node-gyp/bin/node-gyp.js" "$@"
  ret=$?
fi
exit $ret
