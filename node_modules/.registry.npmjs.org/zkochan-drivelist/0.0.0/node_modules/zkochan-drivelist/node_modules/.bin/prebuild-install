#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/prebuild-install/2.3.0/node_modules/prebuild-install/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/prebuild-install/2.3.0/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/prebuild-install/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/zkochan-drivelist/0.0.0/node_modules/prebuild-install/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/zkochan-drivelist/0.0.0/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/zkochan-drivelist/node_modules" "$basedir/node"  "$basedir/../../../prebuild-install/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/prebuild-install/2.3.0/node_modules/prebuild-install/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/prebuild-install/2.3.0/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/prebuild-install/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules:/home/zkochan/src/pnpm/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/zkochan-drivelist/0.0.0/node_modules/prebuild-install/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/zkochan-drivelist/0.0.0/node_modules:/home/zkochan/src/pnpm/pnpm/node_modules/.registry.npmjs.org/zkochan-drivelist/node_modules" node  "$basedir/../../../prebuild-install/bin.js" "$@"
  ret=$?
fi
exit $ret
